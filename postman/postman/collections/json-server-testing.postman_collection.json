                  "            },",
                  "            \"type\": {",
                  "                \"type\": \"string\"",
                  "            },",
                  "            \"address\": {",
                  "                \"type\": \"object\",",
                  "                \"required\": [\"street\", \"suite\", \"city\", \"zipcode\"],",
                  "                \"properties\": {",
                  "                    \"street\": {",
                  "                        \"type\": \"string\"",
                  "                    },",
                  "                    \"suite\": {",
