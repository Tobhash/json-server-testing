          "name": "Get all characters",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "b5c8479c-959a-4d26-ac10-e3113e05d8cc",
                "exec": [
                  "const jsonResponse = pm.response.json();",
                  "",
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "pm.test(\"Response is an array\", function () {",
                  "    pm.expect(jsonResponse).to.be.an('array');",
                  "});",
                  "",
                  "pm.test(\"Response array is not empty\", function () {",
                  "    pm.expect(jsonResponse).to.not.be.empty;",
                  "    pm.expect(jsonResponse.length).to.be.greaterThan(0);",
                  "});",
                  ""
                ],
