                  "pm.test(\"Response is not empty\", function () {",
                  "    pm.expect(pm.response.text()).to.not.be.empty;",
                  "});",
                  "",
                  "pm.test(\"firstName property exists and has the value 'V'\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.firstName).to.exist;",
                  "    pm.expect(jsonData.firstName).to.eql(\"V\");",
                  "});",
                  "",
                  "pm.test(\"lastName property exists and has the value 'Unknown'\", function () {",
                  "    var jsonData = pm.response.json();",
                  "    pm.expect(jsonData.lastName).to.exist;",
                  "    pm.expect(jsonData.lastName).to.eql(\"Unknown\");",
                  "});",
