                  "            \"company\": {",
                  "                \"type\": \"object\",",
                  "                \"required\": [\"companyName\", \"taxNumber\", \"companyPhone\"],",
                  "                \"properties\": {",
                  "                    \"companyName\": {",
                  "                        \"type\": \"string\"",
                  "                    },",
                  "                    \"taxNumber\": {",
                  "                        \"type\": \"string\"",
                  "                    },",
                  "                    \"companyPhone\": {",
                  "                        \"type\": \"string\"",
                  "                    }",
                  "                }",
                  "            },",
                  "            \"cyberware\": {",
                  "                \"type\": \"array\",",
                  "                \"items\": {",
                  "                    \"type\": \"string\"",
